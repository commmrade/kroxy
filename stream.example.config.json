{
  "stream": {
    "servers": {
      "google": {
        "pass_tls_enabled": false,
        "hosts": [
          {
            "host": "google.com",
            "port": 80
          }
        ]
      },
      "google-tls": {
        "pass_tls_enabled": true,
        "hosts": [
          {
            "host": "google.com",
            "port": 443
          }
        ]
      },
      "python": {
        "pass_tls_enabled": false,
        "hosts": [
          {
            "host": "127.0.0.1",
            "port": 9999
          }
        ]
      },
      "httpbin": {
        "pass_tls_enabled": true,
        "hosts": [
          {
            "host": "httpbin.org",
            "port": 443
          }
        ]
      },
      "fuck_it": {
        "pass_tls_enabled": false,
        "hosts": [
          {
            "host": "odnoklassniki.ru",
            "port": 80
          }
        ]
      }
    },


    "port": 8080,
    "pass_to": "httpbin",
    "tls_enabled": false,
    "tls_cert_path": "../tls/certificate.crt",
    "tls_key_path": "../tls/private.key",
    "file_log": "/dev/stdout",
    "format_log": "Client address is $client_addr, $bytes_sent bytes have been sent, $processing_time"
  }
}
