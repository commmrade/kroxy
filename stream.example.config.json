{
  "stream": {
    "servers": {
      "google": {
        "pass_tls_enabled": false,
        "hosts": [
          {
            "host": "google.com",
            "port": 80
          }
        ]
      },
      "google-tls": {
        "pass_tls_enabled": true,
        "hosts": [
          {
            "host": "google.com",
            "port": 443
          }
        ]
      },
      "python": {
        "pass_tls_enabled": false,
        "hosts": [
          {
            "host": "127.0.0.1",
            "port": 9999
          }
        ]
      },
      "httpbin": {
        "pass_tls_enabled": true,
        "hosts": [
          {
            "host": "httpbin.org",
            "port": 443
          }
        ]
      },
      "fuck": {
        "hosts": [
          {
            "host": "127.0.0.1",
            "port": 9090
          }
        ]
      }
    },


    "port": 8080,
    "pass_to": "fuck",
    "pass_read_timeout": 1000,
    "read_timeout": 1000,

    "file_log": "/dev/stdout",
